<p id="notice"><%= notice %></p>

<h1>Listing Matchdays</h1>

<table>
  <thead>
    <tr>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @matchdays.each do |matchday| %>
      <tr>
        <td><%= matchday.matchday %></td>
        <td><%= link_to 'Show', matchday_path(matchday.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
